{
  "name": "tusimo/laravel-closure-job",
  "type": "library",
  "description": "function helper to dispatch a closure job",
  "keywords": ["queue", "closure job", "job"],
  "license": "MIT",
  "authors": [
    {
      "name": "tusimo",
      "email": "ucc862@gmail.com"
    }
  ],
  "require": {
    "illuminate/bus": "^5.2.0",
    "illuminate/support": "^5.2.0",
    "illuminate/queue": "^5.2.0",
    "opis/closure": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "psr-4": {
      "Tusimo\\ClosureJob\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
